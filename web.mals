
# @static since All the Planet's Methods are static
# in a None-static Planet , You can define that a method is static by simply decorating it with @static on top of it .
# We Might update this after to a locked Planet to do some jobs and inherit from it


@static
planet web {


#Main_function_for_create_and_full_file_with_format_(.html)

$als_echo txt fileName isAppend {
	symb = " > "
	if (isAppend == 1){
		symb = " >> "
	}:
	{

	}

	$system("echo "+txt+symb+fileName)
}

$alspage filepath code {
	directory=$f_directory(filepath)
    $f_write(filepath code)
}

$addstyle query  csscodes {
	style = "\n"
	style = style + query + " {\n"  # .className {
	style = style + csscodes 		# color : red ; 
	style = style + "\n }" 			# }
    $f_append(directory+"styleAls.css" style)
}

$addscript jscodes {
	script = "\n"
	script = script + jscodes  	# alert('hello world !'); 
    $f_append(directory+"/scriptAls.js", script)
}

$addevent query eventname jscodes {
	script = "\n" 
	script = script + "$('" + query + "')."+eventname+"(function( {\n"  	#  $('body').load ( function( {
	script = script + jscodes  											# alert('hello world !'); 
	script = script + "\n });" 											# }
    $f_append(directory+"scriptAls.js", script)
}

$texthtml codehtml {
	text = codehtml.replace(" " ,"&nbsp;")
	text = text.replace("<" ,"&lt;")
	text = text.replace(">", "&rt;")
	ret text
}

$open tagname {
    ret "<"+tagname  
}

$close tagname {
    ret "</"+tagname+">" 
}

#Main Tags 

$html code {
    start=$open("html")+">"
    end=$close("html")
    result = start+"\n"+code+"\n"+end 
