$head text_title text_html text_style text_script {
    start=$open("head")+">"
    end=$close("head")
	tools = "\n<link rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css' >"
	tools = tools +"<meta name='viewport' content='width=device-width  initial-scale=1  shrink-to-fit=no'>"
	tools = tools +"\n<script src='https://code.jquery.com/jquery-3.5.1.slim.min.js'></script>" 
	tools = tools +"\n<script src='https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js'></script>"
	tools = tools +"\n<script src='https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js'></script>"
	tools = tools +"\n<script src='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js'></script>"
    text=text_title+tools+"\n"+text_html+"\n"+text_style+"\n"+text_script
    result = start+"\n"+text+"\n"+end 
    ret result
}

